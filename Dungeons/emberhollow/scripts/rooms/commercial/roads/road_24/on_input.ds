

match(<#dungeon.stage>)
@pattern: "tutorial" # {

    match(<#text>, "search")
    @pattern: "\b(?:right|inn)\b" # {

        output("You take the right path.")

        wait(1)

        interact(".road_2_14")

    }
    @pattern: "\b(?:left)\b" # {

        output("You take the left path.")

        wait(1)

        interact(".road_2_23")

    } 

}


