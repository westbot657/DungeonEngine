
[engine:text/match](<#text>)
@pattern: "north" # {
    [engine:interaction/interact](".north_center_street")
}
@pattern: "east" # {
    [engine:interaction/interact](".east_main_street")
}
@pattern: "south" # {
    [engine:interaction/interact](".south_center_street")
}
@pattern: "west" # {
    [engine:interaction/interact](".west_main_street")
}
@pattern: "park" # {
    [engine:interaction/interact](".park")
}
@pattern: "(pawn|shop)" # {
    [engine:interaction/interact](".pawn_shop")
}
