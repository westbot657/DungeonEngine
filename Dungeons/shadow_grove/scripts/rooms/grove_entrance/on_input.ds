


match([engine:text/set_case](<#text>, "lower"))
@pattern: "left" # {
    interact(".left_tunnel")
}
@pattern: "right" # {
    interact(".right_tunnel")
}
@pattern: "door" # {
    interact(".boss_room_door")
}
@pattern: "staff" # {
    interact(".ritual_staff")
}
@pattern: "(exit|leave)" # {
    interact(".exit")
}

