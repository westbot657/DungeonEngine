{
    "fields": {
        "target": {
            "type": "engine:location",
            "required": true
        },
        "travel_message": {
            "type": "engine:text",
            "required": false,
            "default": ""
        },
        "blocked": {
            "type": "engine:boolean",
            "required": false,
            "default": false
        },
        "blocked_message": {
            "type": "engine:text",
            "required": false,
            "default": ""
        }
    },
    "interaction": [
        {
            "@check": {
                "#ref": ".blocked"
            },
            "true": {
                "function": "engine:player/message",
                "message": {
                    "#ref": ".blocked_message"
                }
            },
            "false": {
                "functions": [
                    {
                        "function": "engine:player/message",
                        "message": {
                            "#ref": ".travel_message"
                        }
                    },
                    {
                        "function": "engine:player/set_location",
                        "location": {
                            "#ref": ".target"
                        }
                    }
                ]
            }
        }
    ]
}