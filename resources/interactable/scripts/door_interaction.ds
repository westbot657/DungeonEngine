
if (<.locked>) {
    output(<.lock_message>)
} else {
    $response = input(<.open_message>)

    [engine:text/match]($response)
    @pattern: <%text/pattern/yes> # {
        output(<.travel_message>)
        [engine:player/set_location](<.target>)
    }
    @pattern: <%text/pattern/no> # {
        output(<.disengage_message>)
    }
}
